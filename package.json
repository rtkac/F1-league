{
  "name": "f1-league",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "node": ">= 16.13.0",
    "npm": ">= 8.1.0"
  },
  "scripts": {
    "start:f1-league": "nx serve f1-league-app",
    "build:f1-league": "nx run f1-league-app:build:production",
    "test": "nx test",
    "prettier:apps": "prettier --write --config ./.prettierrc --ignore-path ./.prettierignore \"./apps/**/*.{js,jsx,ts,tsx,json,css,scss,html,md}\"",
    "prettier:libs": "prettier --write --config ./.prettierrc --ignore-path ./.prettierignore \"./libs/**/*.{js,jsx,ts,tsx,json,css,scss,html,md}\"",
    "prettier": "npm run prettier:apps && npm run prettier:libs",
    "lint": "nx run-many --target=lint --all --parallel",
    "release:f1-league:patch": "nx run f1-league-app:version --releaseAs=patch",
    "release:f1-league:minor": "nx run f1-league-app:version --releaseAs=minor",
    "release:f1-league:major": "nx run f1-league-app:version --releaseAs=major",
    "release:f1-league:dry-run": "nx run f1-league-app:version --dry-run",
    "release:push": "git push --follow-tags origin",
    "prepare": "husky install",
    "commit": "cz"
  },
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "i18next": "^22.4.15",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-i18next": "^12.2.0",
    "react-is": "18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "6.8.1",
    "styled-components": "5.3.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@jscutlery/semver": "^2.30.1",
    "@nrwl/cypress": "15.9.2",
    "@nrwl/eslint-plugin-nx": "15.9.2",
    "@nrwl/js": "15.9.2",
    "@nrwl/linter": "15.9.2",
    "@nrwl/react": "15.9.2",
    "@nrwl/vite": "15.9.2",
    "@nrwl/workspace": "15.9.2",
    "@testing-library/react": "14.0.0",
    "@types/node": "18.14.2",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/react-is": "17.0.3",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/coverage-c8": "~0.25.8",
    "@vitest/ui": "^0.25.8",
    "autoprefixer": "10.4.13",
    "babel-plugin-styled-components": "1.10.7",
    "cypress": "^12.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "^8.0.3",
    "jsdom": "~20.0.3",
    "nx": "15.9.2",
    "postcss": "8.4.21",
    "prettier": "^2.8.7",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "3.2.7",
    "typescript": "~4.9.5",
